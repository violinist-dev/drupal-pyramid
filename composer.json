{
  "name": "drupal-pyramid/drupal",
  "description": "Drupal 8 project kickstarter.",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Matthieu SCARSET",
      "role": "Developer",
      "email": "m@matthieuscarset.com",
      "homepage": "https://matthieuscarset.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "require": {
    "php": ">=5.6",
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "^1.6.5",
    "zaporylie/composer-drupal-optimizations": "^1.0",
    "vlucas/phpdotenv": "^2.4",
    "webmozart/path-util": "^2.3",
    "webflo/drupal-finder": "^1.0.0",
    "drupal-composer/drupal-scaffold": "^2.5",
    "drush/drush": "^9.0.0",
    "drupal/console": "^1.0.2",
    "drupal/core": "^8.6.0",
    "drupal/memcache": "^2.0@alpha",
    "drupal/config_split": "^1.4",
    "drupal/config_installer": "^1.8",
    "drupal/config_ignore": "^2.1",
    "drupal/migrate_plus": "^4.0",
    "drupal/migrate_tools": "^4.0",
    "drupal/default_content": "^1.0@alpha",
    "drupal/default_content_extra": "^1.0@beta",
    "drupal/block_visibility_groups": "^1.3",
    "drupal/admin_toolbar": "^1.26",
    "drupal/inline_entity_form": "^1.0@RC",
    "drupal/media_entity_browser": "^2.0",
    "drupal/field_group": "^1.0",
    "drupal/paragraphs": "^1.3",
    "drupal/metatag": "^1.8",
    "drupal/pathauto": "^1.3",
    "drupal/taxonomy_unique": "^2.1",
    "drupal/menu_link_attributes": "^1.0",
    "drupal/menu_manipulator": "^1.6",
    "drupal/ckeditorheight": "^1.3",
    "drupal/editor_advanced_link": "^1.4",
    "drupal/linkit": "^4.3",
    "drupal/search_api": "^1.11",
    "drupal/search_api_autocomplete": "^1.1",
    "drupal/facets": "^1.1",
    "j7mbo/twitter-api-php": "^1.0",
    "drupal/twitter_api_block": "^2",
    "drupal/redirect": "^1.3",
    "drupal/simple_sitemap": "^3.1",
    "drupal/yoast_seo": "^1.5"
  },
  "require-dev": {
    "webflo/drupal-core-require-dev": "^8.6.0",
    "drupal/devel": "^1.2",
    "drupal/coder": "^8.2",
    "drupal/stage_file_proxy": "^1.0@alpha"
  },
  "conflict": {
    "drupal/drupal": "*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "bin-dir": "bin/",
    "sort-packages": false,
    "preferred-install": "dist",
    "discard-changes": true
  },
  "autoload": {
    "classmap": [
      "scripts/composer/ScriptHandler.php"
    ],
    "files": [
      "load.environment.php"
    ]
  },
  "scripts": {
    "drupal-scaffold": [
      "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
    ],
    "pre-install-cmd": [
      "DrupalPyramid\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "pre-update-cmd": [
      "DrupalPyramid\\composer\\ScriptHandler::checkComposerVersion"
    ],
    "post-install-cmd": [
      "DrupalPyramid\\composer\\ScriptHandler::generateFiles"
    ],
    "post-update-cmd": [
      "DrupalPyramid\\composer\\ScriptHandler::generateFiles"
    ],
    "drush": [
      "DrupalPyramid\\composer\\ScriptHandler::doDrush"
    ],
    "generate-salt": [
      "DrupalPyramid\\composer\\ScriptHandler::generateSalt"
    ],
    "generate-files": [
      "DrupalPyramid\\composer\\ScriptHandler::generateFiles"
    ],
    "generate-keys": [
      "DrupalPyramid\\composer\\ScriptHandler::generateOpenSslKeys"
    ],
    "fix-permissions-all": [
      "DrupalPyramid\\composer\\ScriptHandler::fixPermissions"
    ],
    "fix-permissions-keys": [
      "DrupalPyramid\\composer\\ScriptHandler::fixOpenSslKeysPermissions"
    ],
    "fix-permissions": [
      "@fix-permissions-all",
      "@fix-permissions-keys"
    ],
    "reset-files": [
      "DrupalPyramid\\composer\\ScriptHandler::resetFiles"
    ],
    "clean-npm": [
      "DrupalPyramid\\composer\\ScriptHandler::cleanNpm"
    ],
    "clean-keys": [
      "DrupalPyramid\\composer\\ScriptHandler::cleanKeys"
    ],
    "clean-vendors": [
      "DrupalPyramid\\composer\\ScriptHandler::cleanVendors"
    ],
    "clean": [
      "@clean-npm",
      "@clean-keys",
      "@clean-vendors"
    ],
    "code-check-practice": [
      "DrupalPyramid\\composer\\ScriptHandler::phpcbf"
    ],
    "code-check-standard": [
      "DrupalPyramid\\composer\\ScriptHandler::phpcs"
    ],
    "code-review": [
      "@code-check-practice",
      "@code-check-standard"
    ],
    "first-install": [
      "@drupal-scaffold",
      "@generate-files",
      "@generate-salt",
      "@generate-keys",
      "@fix-permissions",
      "@fix-permissions-keys"
    ],
    "run-tests": [
      "DrupalPyramid\\composer\\ScriptHandler::unitTests",
      "DrupalPyramid\\composer\\ScriptHandler::functionalTests"
    ]
  },
  "extra": {
    "composer-exit-on-patch-failure": true,
    "patchLevel": {
      "drupal/core": "-p2"
    },
    "installer-paths": {
      "web/core": [
        "type:drupal-core"
      ],
      "web/libraries/{$name}": [
        "type:drupal-library"
      ],
      "web/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "web/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "web/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "web/themes/custom/{$name}": [
        "type:drupal-theme-custom"
      ],
      "drush/contrib/{$name}": [
        "type:drupal-drush"
      ]
    },
    "drupal-scaffold": {
      "initial": {
        ".editorconfig": "../.editorconfig",
        ".gitattributes": "../.gitattributes"
      },
      "excludes": [
        "sites/default/default.services.yml",
        "sites/development.services.yml",
        "sites/example.settings.local.php"
      ]
    },
    "patches": {
      "drupal/default_content": {
        "#2698425 - Do not reimport existing content": "https://www.drupal.org/files/issues/2018-12-11/default_content-dont-reimport-2698425-109.patch"
      },
      "drupal/paragraphs": {
        "#2901390 - Langcode NULL when saving embed paragraph entity": "https://www.drupal.org/files/issues/paragraphs-missing-langcode-2901390-9.patch"
      }
    }
  }
}
